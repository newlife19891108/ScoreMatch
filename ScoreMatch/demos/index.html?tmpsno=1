        }(function (a) { if ("undefined" != typeof module && module.exports) return function (a) { module.exports = a() }; if ("function" == typeof define && define.amd) return define; if ("undefined" != typeof window) return function (a) { window.MobileDetect = a() }; throw new Error("unknown environment") }());
        var md = new MobileDetect(window.navigator.userAgent);
        var isTouch = (('ontouchstart' in window) || (navigator.msMaxTouchPoints > 0));
        $(function () {
            //header horizontal scroll fix for small screens
            if ($('body').hasClass('fixed-header')){
                $(window).scroll(function () {
                    $('.knet-header .wrapper').css('left', -$(this).scrollLeft() + "px");
                });
            }

            var delay = 700;
            

            // UX ADAY: header main navigation hover
            // desktop?
            if (!md.mobile()) {
                $(".knet-header .nav>ul>li").hover(function () {
                    $(this).siblings().removeClass('active');
                    clearTimeout($(this).data('timeout'));
                    $(this).addClass("active");
                }, function () {
                    var _this = $(this);
                    var t = setTimeout(function () {
                        _this.removeClass("active");
                    }, delay);
                    $(this).data('timeout', t);
                });
            }
            // mobile?
            else {
                $(".knet-header .nav>ul>li").on('touchstart', function (e) {
                    //iş ara değilse
                    if (!$(this).hasClass('is-ara')) {
                        //sub menü kapalıysa
                        if (!$(this).hasClass('active')) {
                            e.preventDefault();
                            $(this).siblings().removeClass('active');
                            $(this).addClass("active");
                        }
                    }
                });
            }


            // UX ADAY:footer navigation hover
            // desktop?
            if (!md.mobile()) {
                $(".footer .nav>ul>li").hover(function () {
                    $(this).siblings().removeClass('active');
                    clearTimeout($(this).data('timeout'));
                    $(this).find('ul').width($(this).parent().width() - 26);
                    $(this).addClass("active");
                }, function () {
                    var _this = $(this);
                    var t = setTimeout(function () {
                        _this.removeClass("active")
                    }, delay);
                    $(this).data('timeout', t);
                });
            }
            // mobile?
            else {
                $(".footer .nav>ul>li").on('touchstart', function (e) {
                    //sub menü kapalıysa
                    if (!$(this).hasClass('active')) {
                        e.preventDefault();
                        $(this).siblings().removeClass('active');
                        $(this).find('ul').width($(this).parent().width() - 26);
                        $(this).addClass("active");
                    }
                });
            }


            // UX ADAY:Header login navigation hover
            // desktop?
            if (!md.mobile()) {
                $(".knet-header .login-control .lgn").hover(function () {
                    $(this).addClass("active");
                }, function () {
                    $(this).removeClass("active");
                });
            }
            // mobile?
            else {
                $(".knet-header .login-control .lgn").on('touchstart', function (e) {
                    $(this).addClass("active").siblings().removeClass('active');
                });
            }


            // UX ADAY:header after login user actions hover
            // desktop?
            if (!md.mobile()) {
                $(".knet-header .login-control .user").hover(function () {
                    $(this).addClass("active");
                }, function () {
                    $(this).removeClass("active");
                });
            }
            // mobile?
            else {
                $(".knet-header .login-control .user").on('touchstart', function (e) {
                    //sub menü kapalıysa
                    if (!$(this).hasClass('active')) {
                        e.preventDefault();
                        $(this).addClass("active").siblings().removeClass('active');
                    }
                });
            }

            //mobile close hovers
            if (md.mobile()) {
                var _headerNav = $(".knet-header .nav>ul>li");
                var _footerNav = $(".footer .nav>ul>li");
                var _headerLogin = $(".knet-header .login-control .lgn");
                var _headerPostLogin = $(".knet-header .login-control .user");
                //header main navigation hover
                $(document).click(function (e) {
                    if (!_headerNav.is(e.target) && _headerNav.has(e.target).length === 0) {
                        _headerNav.removeClass('active');
                    }
                });
                // footer navigation hover
                $(document).click(function (e) {
                    if (!_footerNav.is(e.target) && _footerNav.has(e.target).length === 0) {
                        _footerNav.removeClass('active');
                    }
                });
                // Header login navigation hover
                $(document).click(function (e) {
                    if (!_headerLogin.is(e.target) && _headerLogin.has(e.target).length === 0) {
                        _headerLogin.removeClass('active');
                    }
                });
                // header after login user actions hover
                $(document).click(function (e) {
                    if (!_headerPostLogin.is(e.target) && _headerPostLogin.has(e.target).length === 0) {
                        _headerPostLogin.removeClass('active');
                    }
                });
            }

        });
    </script>
