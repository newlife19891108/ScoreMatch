                yeniisaraparam = "&isaratipi=" + $('[id$=hdnklasikisara]').val();

            var tmpsno = $('[id$=hdmtmpsno]').val();
            if ($('#hdnSite').val() != '') {
                document.location.href = $('#hdnSite').val() + "?ilankodu=" + ilankodu + '&ulkeId=' + ulkekodu + '&sehirId=' + sehirkodu + '&isaramalog=0&sirano=0&sonincelenen=' + sonincelenen + '&tmpsno=' + tmpsno + yeniisaraparam;
            }
        }
        else {
            if (onlinebasvuruURL.substring(0, 4) != 'http')
                onlinebasvuruURL = 'http://' + onlinebasvuruURL;
            msj = '<br>Bu firma başvurularını aşağıda belirtilen web sitesi üzerinden almaktadır. Aşağıdaki linke tıklayarak firmanın web sitesinden ilana başvurabilirsiniz.<br><br>'

