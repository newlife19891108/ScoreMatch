    <script type="text/javascript" language="javascript">
        //Banner
        function CallHandler() {
            $.ajax({
                url: "/WebSite/Banner.ashx?page=2", //page=0 parametresi banner bilgilerinin session'dan alınacağını belirtir.
                headers: { "cache-control": "no-cache" },
                type: 'POST',
                contentType: "application/json; charset=utf-8",
                data: '',
                success: OnComplete,
                error: OnFail
            });
        }

        function OnComplete(result) {
            if (result!="[]" && result!="")
            {
                //var obj = JSON.parse(result);

                $.each(result, function (key, val) {
                    masterPgBanner.style.display = 'block';
                    if (val.BannerLink == '') {
                        masterPgBanner.innerHTML = "<img \" border=\"0\" src=\"" + val.BannerUrl + "\">";
                    }
                    else {
                        masterPgBanner.innerHTML = "<a target=\"_blank\" href=\"" + val.BannerLink + "\"><img \" border=\"0\" src=\"" + val.BannerUrl + "\"></a>";
                    }
                    AddBannerShow(val.BannerId);
                    $('#masterPgBanner').click(function () {
                        AddBannerClick(val.BannerId);
                    });
                });
            }
        }

        function OnFail(result) {
            alert('Request failed');
        }

        //Enf of Banner

        $('document').ready(function () {
            dataLayer.push({'Category':'Funnel','Action':'Basvuru','Label':'İlan Detay','event':'gaEvent'});

            if (kullaniciID > 0)
