        function yazdir(elem) {
            
            Popup($(elem).html());

 
            //window.open('/website/Firma/Ilanlar/ilanYazdir.aspx?ilanno=' + $('[id$=hdnilankodu]').val());

        }
        function Popup(data) 
        {
            var mywindow = window.open('', '', 'height=800,width=800, scrollbars=Yes');
            mywindow.document.write('<html><head><title></title>');
            mywindow.document.write('<style type="text/css">.fontbig{font-size: 15px !important;}</style>')
            mywindow.document.write('<style type="text/css">.Processes {display:none;}</style>')
            /*optional stylesheet*/ //mywindow.document.write('<link rel="stylesheet" href="main.css" type="text/css" />');
            mywindow.document.write('</head><body>');


            if($.browser.msie){
                //mywindow.document.write('<a class="link" href="Javascript:void(0)" onclick="window.print();"><img alt="Yazdır" src="/WebSite/Images/Kariyerim/Cv/cvYazdir.png" align="absmiddle"border="0px"></a>');

                mywindow.document.write(data);
                //mywindow.document.write('</body></html>');
                mywindow.window.location.reload();    // this is the secret ingredient
                mywindow.focus();                     // not sure if this line is necessary
                mywindow.print();

                //this.print();
            }
            if(/chrom(e|ium)/.test(navigator.userAgent.toLowerCase())){
                mywindow.document.write(data);
                mywindow.document.write('</body></html>');
                mywindow.print();
            }
            if($.browser.mozilla){
                mywindow.document.write(data);
                mywindow.document.write('</body></html>');
                mywindow.print();
            }
            //mywindow.print();
            //mywindow.close();

            return true;
        }
        function GetIlanlar(no) {
            var musteriRefNo = getUrlVars()["fno"];
            var profilo = getUrlVars()["pno"];

            loginCallBack = function () {

                if (no != -99) {
                    $("a[id*='AramaSonuc']").attr('href', '/WebSite/isara/profilIlanlar.aspx?musteriRefNo=' + musteriRefNo + '&profilNo=' + profilo);
                } else {
                    $("a[id*='AramaSonuc']").attr('href', '/WebSite/isara/profilIlanlar.aspx?musteriRefNo=' + musteriRefNo);
                }
            };
            loginCallBack();
        }

      

        ilaniDegerlendir = function () {
            loginCallBack = function () {
                var ilankodu = $('[id$=hdnilankodu]').val();
                openModalDiv(encodeURI('/website/isara/ilanDegerlendir.aspx?ilankodu=' + ilankodu + '&firmaAdi=Accenture Danışmanlık Ltd. Şti.&pozisyonAdi=New Graduate Software Developer', '', 650, 375));
            }
            loginCallBack();
        }

        arkadasimaGonder = function () {
            var ilankodu = $('[id$=hdnilankodu]').val();
            openModalDiv('/website/isara/arkadasimaGonder.aspx', 'ilankodu=' + ilankodu, 550, 400);
        }



        sendIlan = function (target) {
            var strTitle = encodeURIComponent("Accenture Danışmanlık Ltd. Şti. - New Graduate Software Developer iş ilanı detayı");
            var strReurl = $("#hdnReUrl").val();
            var strURL = encodeURIComponent('http://www.kariyer.net' + strReurl);

            var strThumb = "";
            var strDesc = encodeURIComponent('Accenture Danışmanlık Ltd. Şti. - New Graduate Software Developer');
            switch (target) {
                case 'facebook':
                    addFacebook(strThumb, strURL, strTitle, strDesc);
                    break;
                case 'twitter':
                    addTwitter(strURL, strTitle);
                    break;
                case 'googlePlus':
                    addGooglePlus(strURL, strTitle);
                    break;
                case 'ff':
                    addFF(strURL, strTitle);
                    break;
                default:
                    break;
            }
        }

        function LoginGerekli(jobrefno) {
            loginCallBack = function () {
                openModalDiv('/Website/isara/YonlendirLoginG.aspx?ilankodusfr=' + jobrefno, '', 400, 120);
            }
            loginCallBack();
        }

        function LoginGereksiz(jobrefno, ysekli) {
            $('a[id*=lnkBasvur]').click(function () {
                openModalDiv('/Website/isara/YonlendirLoginGsiz.aspx?ilankodusfr=' + jobrefno + '&ysekli=' + ysekli, '', 400, 120);
                //addloader('divuyari');
            });
        }

