        if (onlinebasvuruURL == "") {
            if (!islogin) {
                loginCallBack = function () { islogin = true; ilanaBasvur(); };
                openModalDiv('/website/kariyerim/loginAjax.aspx?basvur=1', '', 700, 250);
                return;
