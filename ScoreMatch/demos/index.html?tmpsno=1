            CallHandler(); //Banner

            setTimeout(function(){
                if($('#lnkBasvurAlt') == undefined)
                {
                    $('#btnUstBasvur').hide();
                }
                else
                {
                    var href=$('#lnkBasvurAlt').attr('href');
                    if(!(href.indexOf('void') > -1))
                    {
                        if(href.indexOf('javascript') > -1)
                            $('#btnUstBasvur').attr('onclick',href);
                        else
                        { $('#btnUstBasvur').attr('onclick','javascript:window.location=\'' + href + '\'');
                        }
                    }
                }
            },500);

            setTimeout(function(){
                if($('#lnkBasvurAlt') == undefined)
                {
                    $('#btnUstBasvur').hide();
                }
                else
                {
                    var href=$('#lnkBasvurAlt').attr('href');
                    if(!(href.indexOf('void') > -1))
