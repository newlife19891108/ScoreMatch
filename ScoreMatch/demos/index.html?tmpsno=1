            }
    }

    function bindswf(SwfURL) {
        SwfURL = '/UploadFiles/Clients/Cingil/' + SwfURL;
        if ($.browser.msie) {
            var result = '';
            result += "<object id=\"\" classid=\"clsid:D27CDB6E-AE6D-11cf-96B8-444553540000\" width=\"39\" height=\"39\">";
            result += "	<param name=\"movie\" value=\"" + SwfURL + "\" />";
            result += "		<!--[if !IE]>-->";
            result += "	<object type=\"application/x-shockwave-flash\" data=\"" + SwfURL + "\" width=\"39\" height=\"39\">";
            result += "	<!--<![endif]-->";
            result += "	<div>";
            result += "		<p><a href=\"http://www.adobe.com/go/getflashplayer\"><img src=\"http://www.adobe.com/images/shared/download_buttons/get_flash_player.gif\" alt=\"Get Adobe Flash player\" /></a></p>";
            result += "	</div>";
            result += "	<!--[if !IE]>-->";
            result += "	</object>";
            result += "	<!--<![endif]-->";
            result += "</object>";

            $('#tdSwf').html(result);
        } else {
            $('#divSpeak').attr('style', 'float:right;padding-right:15px;padding-top:2px;');
            swfobject.embedSWF(SwfURL, 'tdSwf', '39', '39', '9.0.0');
        }
    }

